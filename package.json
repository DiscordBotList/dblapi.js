{
  "name": "@top-gg/sdk",
  "version": "3.1.1",
  "description": "Official Top.gg Node SDK",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:ci": "npm i --also=dev && tsc",
    "docs": "npm run build && npm run md",
    "md": "jsdoc2md ./dist/structs/Api.js > ./docs/Api.md && jsdoc2md ./dist/structs/Webhook.js > ./docs/Webhook.md",
    "prepublishOnly": "npm run build",
    "lint": "eslint '*/**/*.ts'",
    "lint:ci": "eslint --output-file eslint_report.json --format json '*/**/*.ts'",
    "prepare": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/top-gg/node-sdk.git"
  },
  "author": "Top.gg <support@top.gg> (https://top.gg)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/top-gg/node-sdk/issues"
  },
  "homepage": "https://topggjs.rtfd.io",
  "devDependencies": {
    "@top-gg/eslint-config": "^0.0.2",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "jsdoc-to-markdown": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "raw-body": "^2.4.1"
  },
  "types": "./dist/index.d.ts"
}
